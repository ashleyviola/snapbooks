<section class="content-container">
    <div id="return-dashboard-container">
        <a class="return-to-dashboard" href="/dashboard"><i class="fas fa-long-arrow-alt-left"></i> Return to Dashboard</a>
    </div>
    <div>
        <h2 id="secondary-subhead">Client List</h2>
        <ul class="fa-ul" id="client-list">
            {{#each clients as |client|}}
            <li><a id="client-link" href="/dashboard/clients/{{client.id}}"><span class="fa-li"><i class="fas fa-chevron-right"></i></span>{{client.company_name}}</a></li>
            {{/each }}
        </ul>
    </div>
</section>