<section class="content-container">
    <div>
        <div id="return-dashboard-container">
        <a class="return-to-dashboard" href="/dashboard"><i class="fas fa-long-arrow-alt-left"></i> Return to Dashboard</a>
        </div>
        <form class="create-client-form"id="create-form">
            <h2 id="secondary-subhead">Create Client</h2>
            <label id="secondary-label">Company</label>
            <input type="text" id="client-company" name="client-company">
            <label id="secondary-label">Address</label>
            <input type="text" id="client-address" name="client-address">
            <label id="secondary-label">City</label>
            <input type="text" id="client-city" name="client-city">
            <label id="secondary-label">State</label>
            <input type="text" id="client-state" name="client-state">
            <label id="secondary-label">Zip Code</label>
            <input type="number" id="client-zip" name="client-zip">
            <label id="secondary-label">Contact First Name</label>
            <input type="text" id="client-first-name" name="client-first-name">
            <label id="secondary-label">Contact Last Name</label>
            <input type="text" id="client-last-name" name="client-last-name">
            <label id="secondary-label">Email</label> 
            <input type="email" id="client-email" name="client-email">
            <label id="secondary-label">Phone Number</label> 
            <input type="tel" id="client-tel" name="client-tel">
            <button type="submit">Create Client</button>
        </form>
    </div>
</section>
<script src="/javascript/add-client.js"></script>

